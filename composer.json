{
    "name": "joni-jones/yii2-fuploader",
    "description": "BlueImp file upload widget for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","file upload","jquery file upload","blueimp plugin"],
    "license": "MIT",
    "stability": "stable",
    "authors": [
        {
            "name": "Ievgen Sentiabov",
            "email": "ievgen.sentiabov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "bower-asset/jquery.ui": "1.11.*",
        "bower-asset/jquery-file-upload": "9.9.*"
    },
    "autoload": {
        "psr-4": {
            "jones\\fuploader\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
